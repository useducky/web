{
  "name": "ducky-web",
  "version": "0.0.1",
  "repository": "https://github.com/useducky/web",
  "description": "The website for Ducky.",
  "main": "index.js",
  "scripts": {
    "start": "NODE_PATH=$NODE_PATH:./shared node server/index.jsx",
    "get-ip": "ifconfig | grep -Eo 'inet (addr:)?([0-9]*\\.){3}[0-9]*' | grep -Eo '([0-9]*\\.){3}[0-9]*' | grep -v '127.0.0.1'",
    "public-server": "DIST_PATH=http://$(npm run -s get-ip):8080/ npm run start",
    "dev": "webpack-dev-server --progress --color --config=webpack.client.config.js",
    "dev-server": "npm run dev && sleep 10 && DIST_PATH=http://localhost:8080/ npm run start",
    "predev": "npm run clean",
    "clean": "rm -rf dist/* && rm -f shared/webpack-assets.json",
    "build": "NODE_ENV=production webpack --config=webpack.client.config.js -p",
    "prebuild": "npm run clean"
  },
  "author": "Paddy <paddy@impractical.co>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.7.0",
    "css-modules-require-hook": "^2.0.1",
    "express": "^4.13.3",
    "history": "^1.9.1",
    "immutable": "^3.7.5",
    "object-assign": "^4.0.1",
    "postcss": "^5.0.10",
    "postcss-modules-extract-imports": "^1.0.0",
    "postcss-modules-local-by-default": "^1.0.0",
    "postcss-modules-scope": "^1.0.0",
    "postcss-modules-values": "^1.1.1",
    "react": "^15.0.0-rc.2",
    "react-dom": "^15.0.0-rc.2",
    "react-redux": "^4.0.0",
    "react-router": "1.0.0-rc3",
    "redux": "^3.0.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.1.0",
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.2",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.21.0",
    "eslint": "^1.4.3",
    "eslint-plugin-react": "^3.3.2",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.3",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^2.1.5",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.1",
    "webpack-dev-server": "^1.11.0"
  }
}
